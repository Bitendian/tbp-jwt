{
    "name": "bitendian/tbp-jwt",
    "type": "library",
    "description": "Light JWT implementation for TBP",
    "keywords": ["tbp", "client", "server", "rest", "jwt", "auth"],
    "homepage": "http://www.bitendian.com/en/portfolio/tbp-5/",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Bitendian S.L.",
            "email": "info@bitendian.com",
            "homepage": "http://www.bitendian.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Bitendian/tbp-jwt.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "bitendian/tbp": "1.0.18",
        "firebase/php-jwt" : "*",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Bitendian\\JWT\\Client\\": "src/Client/src/",
            "Bitendian\\JWT\\Server\\": "src/Server/src/"
        }
    }
}
